	<parameter name="quadroorcamento_id" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
     p.`id` AS p_id,
     p.`ld` AS p_ld,
     p.`metodo` AS p_metodo,
     p.`nome` AS p_nome,
     p.`unidadeMedida` AS p_unidadeMedida,
     q.`nome` AS q_nome,
     q.`valorTotal` AS q_valorTotal,
     q.`orcamento_id` AS q_orcamento_id,
     qp.`parametros_id` AS qp_parametros_id,
     qp.`QuadroOrcamento_id` AS qp_QuadroOrcamento_id
FROM
     `quadroorcamento_parametro` qp INNER JOIN `quadroorcamento` q ON qp.`quadroorcamento_id` = q.`id`
     INNER JOIN `parametro` p ON qp.`parametros_id` = p.`id`
WHERE
     q.id = $P{quadroorcamento_id}]]>
	</queryString>
